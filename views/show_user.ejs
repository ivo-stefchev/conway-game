<% if (login) { %>
    <div>
        Logged in as <strong><%= username %></strong>
    </div>

    <div>
        <a href="/logout">Logout</a>
    </div>
<% } else { %>
    <div>
        <a href="/login">Login</a>
    </div>

    <div>
        <a href="/register">Register</a>
    </div>
<% } %>
<div>
    <a href="/">Home</a>
</div>
